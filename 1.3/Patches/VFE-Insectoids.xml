<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Factions Expanded - Insectoids</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VFE_Gun_Plasmacutter"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>Pistol</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VFE_Gun_Plasmapiercer"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>Rifle</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VFE_Gun_PlasmabeamRifle"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>Rifle</gmclass>
						</li>
					</value>
				</li>
        <li Class ="PatchOperationAddModExtension">
          <xpath>Defs/ThingDef[defName="VFE_Gun_PlasmaScattergun"]</xpath>
          <value>
            <li Class="SK_WeaponMastery.DefModExtension_Mastery">
              <gmclass>Shotgun</gmclass>
            </li>
          </value>
        </li>
        <li Class ="PatchOperationAddModExtension">
          <xpath>Defs/ThingDef[defName="VFE_Plasmasaw"]</xpath>
          <value>
            <li Class="SK_WeaponMastery.DefModExtension_Mastery">
              <gmclass>Bladed</gmclass>
            </li>
          </value>
        </li>
			</operations>
		</match>
	</Operation>
</Patch>