<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Weapons Expanded - Coilguns</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VWE_Gun_GaussRifle"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>gauss rifle</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VWE_Gun_GaussMagnum"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>gauss pistol</gmclass>
						</li>
					</value>
				</li>
				<li Class ="PatchOperationAddModExtension">
					<xpath>Defs/ThingDef[defName="VWE_Gun_GaussLance"]</xpath>
					<value>
						<li Class="SK_WeaponMastery.DefModExtension_Mastery">
							<gmclass>gauss sniper rifle</gmclass>
						</li>
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>