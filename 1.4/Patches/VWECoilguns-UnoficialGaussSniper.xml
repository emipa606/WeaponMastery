<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>VWE Coilguns - Unnoficial Gauss Sniper</li>
    </mods>
    <match Class="PatchOperationSequence">
      <operations>
        <li Class="PatchOperationAddModExtension">
          <xpath>Defs/ThingDef[defName="VWE_Gun_GaussSniper"]</xpath>
          <value>
            <li Class="WeaponMastery.DefModExtension_Mastery">
              <gmclass>Sniper Rifle</gmclass>
            </li>
          </value>
        </li>
      </operations>
    </match>
  </Operation>
</Patch>